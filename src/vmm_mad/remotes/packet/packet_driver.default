<!--
 Default configuration attributes for the PACKET driver
 (all domains will use these values as defaults)
 Valid atributes are: PROJECT, FACILITY, PLAN, BILLING_CYCLE, OS
  
 Use XML syntax to specify defaults, note elements are UPCASE
 Example:
 <TEMPLATE>
   <PUBLIC_CLOUD>
     <PACKET_TOKEN>token</PACKET_TOKEN>
     <PLAN>baremetal_0</PLAN>
   </PUBLIC_CLOUD>
 </TEMPLATE>
-->

<TEMPLATE>
  <PUBLIC_CLOUD>
    <PLAN>baremetal_0</PLAN>
  </PUBLIC_CLOUD>
</TEMPLATE>
