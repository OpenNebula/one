    "dev": "NODE_ENV=development npm run build:remotes && NODE_ENV=development npm run build-client && cross-env NODE_ENV=development babel-node ./src/server/index.js",
    "build-client": "rimraf dist/client && webpack --config ./webpack.config.prod.client.js",
    "build-server": "rimraf dist/index.js dist/index.worker.js && webpack --config ./webpack.config.prod.server.js",
    "build:um": "rimraf dist/modules/UtilsModule && BUILD_TARGET=remote webpack --config ./src/modules/utils/webpack.config.prod.utils.js",
    "build:cm": "rimraf dist/modules/ConstantsModule && BUILD_TARGET=remote webpack --config ./src/modules/constants/webpack.config.prod.constants.js",
    "build:ctm": "rimraf dist/modules/ContainersModule && BUILD_TARGET=remote webpack --config ./src/modules/containers/webpack.config.prod.containers.js",
    "build:cpm": "rimraf dist/modules/ComponentsModule && BUILD_TARGET=remote webpack --config ./src/modules/components/webpack.config.prod.components.js",
    "build:mm": "rimraf dist/modules/ModelsModule && BUILD_TARGET=remote webpack --config ./src/modules/models/webpack.config.prod.models.js",
    "build:fm": "rimraf dist/modules/FeaturesModule && BUILD_TARGET=remote webpack --config ./src/modules/features/webpack.config.prod.features.js",
    "build:hm": "rimraf dist/modules/HooksModule && BUILD_TARGET=remote webpack --config ./src/modules/hooks/webpack.config.prod.hooks.js",
    "build:pm": "rimraf dist/modules/ProvidersModule && BUILD_TARGET=remote webpack --config ./src/modules/providers/webpack.config.prod.providers.js",
    "build:remotes": "npm run build:um && npm run build:cm && npm run build:ctm && npm run build:cpm && npm run build:mm && npm run build:fm && npm run build:hm && npm run build:pm",
    "build": "NODE_ENV=production npm run build-client && NODE_ENV=production npm run build-server && NODE_ENV=production npm run build:remotes",
    "start": "NODE_ENV=production node ./dist/index.js",
    "pot": "node potfile.js",
    "po2json": "node po2json.js",
    "lint": "npm run lint-client && npm run lint-server",
    "lint-client": "eslint ./src/client/",
    "lint-server": "eslint ./src/server/",
    "lint-fix": "eslint ./src/ --fix"
  },
