{
  "name": "FireEdge",
  "version": "1.0.0",
  "description": "Opennebula FireEdge",
  "main": "src/index.js",
  "scripts": {
    "dev": "rimraf dist/ && cross-env NODE_ENV=development babel-node ./src/server/index.js",
    "build-front": "rimraf dist/client && webpack --config ./webpack.config.prod.client.js",
    "build-server": "rimraf dist/index.js && rimraf dist/fireedge-server.conf && webpack --config ./webpack.config.prod.server.js",
    "build": "npm run build-front && npm run build-server",
    "start": "cross-env NODE_ENV=production babel-node ./dist/index.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless --browser chrome --spec \"cypress/integration/**/*.spec.js\"",
    "pot": "node potfile.js",
    "po2json": "node po2json.js",
    "lint": "eslint ./src/client/",
    "lint-fix": "eslint ./src/client/ --fix"
  },
  "author": "opennebula.io",
  "license": "ISC",
  "engines": {
    "node": ">=10.21.0"
  },
  "keywords": [
    "flow",
    "vms",
    "opennebula"
  ],
