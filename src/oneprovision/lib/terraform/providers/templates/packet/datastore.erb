resource "packet_volume" "device_<%= obj['ID'] %>" {
    description   = "<%= obj['ID'] %>_volume"
    facility      = "<%= provision['FACILITY'] %>"
    project_id    = "<%= provision['PACKET_PROJECT'] %>"
    plan          = "<%= provision['PLAN'] %>"
    size          = "<%= obj['TOTAL_MB'] %>"
    billing_cycle = "hourly"
}

