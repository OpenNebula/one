terraform {
  required_providers {
    scaleway = {
      source = "scaleway/scaleway"
    }
  }
  required_version = ">= 0.13"
}

provider "scaleway" {
  access_key = "<%= conn['ACCESS_KEY'] %>"
  secret_key = "<%= conn['SECRET_KEY'] %>"
  project_id = "<%= conn['PROJECT_ID'] %>"
  zone       = "<%= conn['ZONE'] %>"
  region     = "<%= conn['REGION'] %>"
}
