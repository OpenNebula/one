              <xs:element name="SERVER_ID" type="xs:integer" minOccurs="0" maxOccurs="1"/>
              <xs:element name="ZONE_ID" type="xs:integer" minOccurs="0" maxOccurs="1"/>
            </xs:all>
          </xs:complexType>
