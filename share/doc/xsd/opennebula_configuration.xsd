        <xs:element name="INHERIT_DATASTORE_ATTR" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="INHERIT_IMAGE_ATTR" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
