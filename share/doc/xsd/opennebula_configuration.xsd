            <xs:pattern value="[yY][eE][sS]" />
            <xs:pattern value="[nN][oO]" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <xs:element name="VM_USE_OPERATIONS" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>

        <xs:element name="VNC_PORTS" minOccurs="0" maxOccurs="1">
