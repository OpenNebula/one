        </xs:element>

        <xs:element name="MAX_CONN" type="xs:integer" minOccurs="0" maxOccurs="1"/>
        <xs:element name="MAX_CONN_BACKLOG" type="xs:integer" minOccurs="0" maxOccurs="1"/>
