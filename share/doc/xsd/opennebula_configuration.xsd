              </xs:element>
              <xs:element name="DS_LIVE_MIGRATION" minOccurs="0" maxOccurs="1">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                  <xs:pattern value="[yY][eE][sS]" />
