              <xs:element name="USER" minOccurs="0" maxOccurs="1" type="xs:string"/>
              <xs:element name="TIMEOUT" minOccurs="0" maxOccurs="1" type="xs:integer"/>
            </xs:all>
          </xs:complexType>
        </xs:element>

        <xs:element name="DEFAULT_AUTH" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
