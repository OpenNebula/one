        update|quota|batchquota|umask|delete|passwd|chgrp|addgroup| \
        delgroup|chauth|show)
            _complete $cmd
        ;;
        defaultquota)
            COMPREPLY=( $(compgen -A file -- "${cur}") )
            return 0
        ;;
        esac
    elif [ "$COMP_CWORD" == 3 ]; then
        case "$pprev" in
        chgrp|addgroup|delgroup)
            _complete onegroup
        ;;
        update|quota|batchquota)
            COMPREPLY=( $(compgen -A file -- "${cur}") )
            return 0
        ;;
        esac
    fi
}

